{
  "name": "codeboard",
  "displayName": "codeboard",
  "publisher": "vsls-contrib",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/vsls-contrib/codeboard.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openCodeboard"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openCodeboard",
        "title": "Codeboard: Open"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm run build:webview && npm run build:extension",
    "build:extension": "tsc -p ./",
    "build:webview": "cd webview && npm run build",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@octokit/graphql": "^4.5.4",
    "mobx": "^5.14.2"
  }
}
